{
  "name": "eslint-plugin-cypress-strict",
  "version": "0.0.12",
  "description": "Strict & opinionated ESLint rules for writing Cypress tests",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "cypress"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kiwicom/eslint-plugin-cypress-strict.git"
  },
  "license": "MIT",
  "author": "Strajk",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "jest",
    "test:coverage": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "jest": {
    "transform": {
      "^.+\\.js?$": "babel-jest"
    }
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "@pika/plugin-build-node": "0.3.16",
    "@pika/plugin-standard-pkg": "0.3.16",
    "babel-jest": "24.8.0",
    "coveralls": "3.0.3",
    "eslint": "5.16.0",
    "eslint-config-standard": "13.0.1",
    "eslint-config-strajk": "0.0.4",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-standard": "4.0.0",
    "jest": "24.8.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg",
        {
          "exclude": [
            "**/__tests__/**/*"
          ]
        }
      ],
      [
        "@pika/plugin-build-node"
      ]
    ]
  }
}
